#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lamha1368/freelankakot-inkathon/freelankakot/packages/contracts/node_modules/.pnpm/ts-node@10.9.1_@types+node@18.18.3_typescript@5.2.2/node_modules/ts-node/dist/node_modules:/home/lamha1368/freelankakot-inkathon/freelankakot/packages/contracts/node_modules/.pnpm/ts-node@10.9.1_@types+node@18.18.3_typescript@5.2.2/node_modules/ts-node/node_modules:/home/lamha1368/freelankakot-inkathon/freelankakot/packages/contracts/node_modules/.pnpm/ts-node@10.9.1_@types+node@18.18.3_typescript@5.2.2/node_modules:/home/lamha1368/freelankakot-inkathon/freelankakot/packages/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lamha1368/freelankakot-inkathon/freelankakot/packages/contracts/node_modules/.pnpm/ts-node@10.9.1_@types+node@18.18.3_typescript@5.2.2/node_modules/ts-node/dist/node_modules:/home/lamha1368/freelankakot-inkathon/freelankakot/packages/contracts/node_modules/.pnpm/ts-node@10.9.1_@types+node@18.18.3_typescript@5.2.2/node_modules/ts-node/node_modules:/home/lamha1368/freelankakot-inkathon/freelankakot/packages/contracts/node_modules/.pnpm/ts-node@10.9.1_@types+node@18.18.3_typescript@5.2.2/node_modules:/home/lamha1368/freelankakot-inkathon/freelankakot/packages/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
fi
